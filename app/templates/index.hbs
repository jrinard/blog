<div class="pull-right button-up">
{{new-post savePost2="savePost3"}} <br>
</div>

<h2>Home</h2>




  {{#each model.posts as |post|}}
  {{post-tile post=post}}
  {{/each}}



<div id="comment-bottom">

  {{#each model.comments as |comment|}}
    <li>{{comment.rating}} - {{comment.content}} - by {{comment.author}}</li>
  {{/each}}

</div>
